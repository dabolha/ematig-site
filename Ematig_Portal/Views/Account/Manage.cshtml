@using Ematig_Portal.Models;
@using Microsoft.AspNet.Identity;

@{
    ViewBag.Title = "Alterar dados";
}

<header class="page-header">
    <div class="banner">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <h2>@ViewBag.Title</h2>
                </div>
            </div>
        </div>
    </div>
</header>

<section class="section-intro section-login">
    <div class="container container_padding box_size1">
        <div class="row">
            <div class="col-md-8 login-form">
                <section id="loginForm">
                    
                    @Html.Partial("_ChangeInfoPartial")

                    <section id="externalLogins">
                        @Html.Action("RemoveAccountList")
                    </section>
                </section>
            </div>
        </div>
    </div>
</section>